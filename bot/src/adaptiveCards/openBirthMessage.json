{
  "$schema": "http://adaptivecards.io/schemas/adaptive-card.json",
  "type": "AdaptiveCard",
  "version": "1.5",
  "body": [
    {
      "type": "TextBlock",
      "text": "메시지가 도착했습니다.",
      "weight": "Bolder",
      "wrap": true
    }
  ],
  "actions": [
    {
      "type": "Action.Execute",
      "title": "OK",
      "verb": "openBirthMessage",
      "fallback": "Action.Submit",
      "data": {
        "messageId": "${messageId}",
        "birthDate": "${birthDate}",
        "username": "${username}"
      }
    }
  ]
}